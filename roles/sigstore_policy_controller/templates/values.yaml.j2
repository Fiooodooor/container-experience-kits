webhook:
  env:
{% if "https_proxy" in proxy_env %}
    https_proxy: "{{ proxy_env.https_proxy }}"
{% endif %}
{% if "no_proxy" in proxy_env %}
    no_proxy: "{{ proxy_env.no_proxy }}"
{% endif %}
{% if registry_enable|d(false) %}
  registryCaBundle:
    name: ca-bundle-config
    key: ca-bundle.crt
{% endif %}
