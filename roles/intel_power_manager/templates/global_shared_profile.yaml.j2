---
apiVersion: "power.intel.com/v1"
kind: PowerProfile
metadata:
  name: shared-global
  namespace: {{ intel_power_manager_namespace }}
spec:
  name: "shared-global"
  max: {{ intel_power_manager.global_max_frequency }}
  min: {{ intel_power_manager.global_min_frequency }}
  epp: "power"
  governor: {{ intel_power_manager.global_pstate_governor }}
